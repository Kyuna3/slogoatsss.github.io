---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>SLOGOATSSS</title>
	<link href="http://slogoatsss.github.io/deckliste/atom.xml" rel="self"/>
	<link href="http://slogoatsss.github.io/deckliste"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://slogoatsss.github.io/deckliste</id>
	<author>
		<email>slogoatsss@gmail.com</email>
	</author>

	{% for post in site.postsDeckliste %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://slogoatsss.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://slogoatsss.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
